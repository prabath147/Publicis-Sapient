{
	"name": "online-pharmacy-frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@emotion/react": "^11.10.0",
		"@fortawesome/fontawesome": "^1.1.8",
		"@fortawesome/free-regular-svg-icons": "^6.2.0",
		"@fortawesome/free-solid-svg-icons": "^6.1.2",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@mantine/core": "^5.2.3",
		"@mantine/dates": "^5.2.3",
		"@mantine/ds": "^5.1.6",
		"@mantine/form": "^5.2.0",
		"@mantine/hooks": "^5.2.3",
		"@mantine/modals": "^5.2.3",
		"@mantine/notifications": "^5.1.6",
		"@reduxjs/toolkit": "^1.8.3",
		"@tabler/icons": "^1.83.1",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.4.1",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"axios": "^0.27.2",
		"buffer": "^6.0.3",
		"css-loader": "^6.7.1",
		"dayjs": "^1.11.5",
		"exceljs": "^4.3.0",
		"msw": "^0.45.0",
		"net": "^1.0.2",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-custom-scrollbars-2": "^4.5.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.34.2",
		"react-icons": "^4.4.0",
		"react-jwt": "^1.1.6",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"react-scripts": "5.0.1",
		"react-webcam": "^7.0.1",
		"redux-thunk": "^2.4.1",
		"sockjs-client": "^1.6.1",
		"stompjs": "^2.3.3",
		"tabler-icons-react": "^1.54.0",
		"tesseract.js": "^3.0.2",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"test-coverage": "npm run test -- --coverage --coverageDirectory=coverage/jest --watchAll=false --testResultsProcessor jest-sonar-reporter || exit 0",
		"lint": "eslint src",
		"lint:report": "npm run lint -- -f json -o eslint-report.json",
		"lint-fix": "eslint src --fix"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"root": true,
		"ignorePatterns": "src/__tests__/*"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.18.6",
		"@types/jest": "^28.1.8",
		"@types/node": "^18.7.9",
		"@types/react-test-renderer": "^18.0.0",
		"@types/redux-mock-store": "^1.0.3",
		"@types/sockjs-client": "^1.5.1",
		"@types/stompjs": "^2.3.5",
		"@typescript-eslint/eslint-plugin": "^5.32.0",
		"@typescript-eslint/parser": "^5.32.0",
		"eslint": "^8.21.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jest": "^28.1.3",
		"jest-sonar-reporter": "^2.0.0",
		"react-test-renderer": "^18.2.0",
		"redux-mock-store": "^1.5.4",
		"resize-observer-polyfill": "^1.5.1",
		"sonarqube-scanner": "^2.8.1",
		"typescript": "^4.7.4",
		"use-resize-observer": "^9.0.2"
	},
	"jest": {
		"coverageReporters": [
			"lcov",
			"text",
			"cobertura"
		],
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80,
				"statements": 80
			}
		},
		"collectCoverageFrom": [
			"!src/index.tsx",
			"!src/__tests__/**"
		],
		"resetMocks": false
	}
}